<?xml version="1.0"?>

<PropertyList>

 <path>exhaust.ac</path>
 
   <animation> 
      <type>shader</type>
      <shader>heat-haze</shader>
      <object-name>noshadow.Exhaust</object-name>
      <object-name>noshadow.Exhaust3</object-name>
      <factor>0.12</factor>
      <speed-prop>engines/engine[1]/n1</speed-prop>
    </animation>
    
	
	<animation> 
      <type>blend</type>
      <object-name>noshadow.Exhaust3</object-name>
      <property>engines/engine[1]/n1</property>
      <factor>.01</factor>
      <offset>0.45</offset>
      <max>0.9</max>
    </animation>
    
	<animation> 
      <type>blend</type>
      <object-name>noshadow.Exhaust</object-name>
      <property>engines/engine[1]/n1</property>
      <factor>-0.01</factor>
      <offset>1</offset>
      <max>0.99</max>
      <min>0.4</min>
    </animation>
    
	<animation> 
      <type>scale</type>
      <object-name>noshadow.Exhaust</object-name>
      <property>engines/engine[1]/n1</property>
      <x-factor>0.01</x-factor>
      <x-offset>0.5</x-offset>
      <x-max>2</x-max>
      <x-min>0.5</x-min>
      <y-factor>0</y-factor>
      <y-offset>1</y-offset>
      <z-factor>0</z-factor>
      <z-offset>1</z-offset>
    </animation>
   
   <animation> 
      <type>translate</type>
      <object-name>noshadow.Exhaust3</object-name>
      <property>engines/engine[1]/n1</property>
      <factor>-0.005</factor>
      <offset-m>0</offset-m>
      <max>3</max>
      <axis> 
        <x>1</x>
        <y>0</y>
        <z>0</z>
      </axis>
    </animation>
    
	<animation> 
      <type>scale</type>
      <object-name>noshadow.Exhaust3</object-name>
      <property>engines/engine[1]/n1</property>
      <x-factor>0.01</x-factor>
      <x-offset>0.5</x-offset>
      <x-max>2</x-max>
      <x-min>0.5</x-min>
      <y-factor>-0.01</y-factor>
      <y-offset>1.45</y-offset>
      <y-max>1</y-max>
      <y-min>-1</y-min>
      <z-factor>-0.01</z-factor>
      <z-offset>1.45</z-offset>
      <z-max>1</z-max>
      <z-min>-1</z-min>
    </animation>
    
	<animation> 
      <type>select</type>
      <object-name>noshadow.Exhaust3</object-name>
      <condition> 
        <less-than> 
          <property>velocities/airspeed-kt</property>
          <value>50</value>
        </less-than>
      </condition>
    </animation>
    
</PropertyList>
