    <factor>8</factor>
    <offset-deg>0</offset-deg>
    <axis>
      <x1-m>23.52</x1-m>
      <y1-m>3.96</y1-m>
      <z1-m>-0.19</z1-m>
      <x2-m>23.77</x2-m>
      <y2-m>9.78</y2-m>
