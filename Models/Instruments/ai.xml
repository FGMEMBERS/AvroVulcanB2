<?xml version="1.0"?>
  <!-- Attitude -->
  <PropertyList> 
    <path>ai.ac</path>
    <animation>
		<type>rotate</type>
		<object-name>flag</object-name>
   		<property>systems/electrical/outputs/MRG</property>
		<interpolation>
			<entry>
				<ind>0</ind>
				<dep>90</dep>
			</entry>
			<entry>
				<ind>110</ind>
				<dep>0</dep>
			</entry>
		</interpolation>
		<center>
			<x-m>0.000943692</x-m>
			<y-m>-0.0293421</y-m>
			<z-m>0.0310995</z-m>
		</center>
      	<axis> 
       		<x>1</x>
        	<y>0</y>
        	<z>0</z>
      	</axis>
    </animation>
	<animation> 
      <type>rotate</type>
      <object-name>wingline</object-name>
      <property>orientation/roll-deg</property>
		<factor>-1</factor>
		<min-deg>-30</min-deg>
		<max-deg>30</max-deg>
      <axis> 
        <x>1</x>
        <y>0</y>
        <z>0</z>
      </axis>
    </animation>
   	<animation> 
      <type>rotate</type>
      <object-name>horizon_line</object-name>
      <property>instrumentation/attitude-indicator/horizon-offset-deg</property>
		<factor>-1</factor>
		<min-deg>-10</min-deg>
		<max-deg>10</max-deg>
      <axis> 
        <x>1</x>
        <y>0</y>
        <z>0</z>
      </axis>
    </animation>
   	<animation> 
      <type>rotate</type>
      <object-name>ai_offset_knob</object-name>
      <property>instrumentation/attitude-indicator/horizon-offset-deg</property>
		<factor>-10</factor>
      <axis> 
        <x>1</x>
        <y>0</y>
        <z>0</z>
      </axis>
      <center> 
        <x-m>0.00890263</x-m>
        <y-m>0.0374334</y-m>
        <z-m>-0.0416821</z-m>
      </center>
    </animation>
	<animation> 
      <type>rotate</type>
      <object-name>bank_circle</object-name>
      <property>orientation/roll-deg</property>
      <interpolation>
		<entry>
			<ind>-90</ind>
			<dep>90</dep>
		</entry>
		<entry>
			<ind>90</ind>
			<dep>-90</dep>
		</entry>
      </interpolation>
      <axis> 
        <x>1</x>
        <y>0</y>
        <z>0</z>
      </axis>
		<center>
			<x-m>0</x-m>
			<y-m>0</y-m>
			<z-m>0</z-m>
		</center>
    </animation>
<animation> 
	<type>translate</type>
	<object-name>pitch_ladder</object-name>
	<property>orientation/pitch-deg</property>
	<interpolation> 
		<entry>
			<ind>-180</ind>
			<dep>0.03</dep>
		</entry>
		<entry>
			<ind>-90</ind>
			<dep>0.03</dep>
		</entry>
		<entry>
			<ind>-40</ind>
			<dep>0.020</dep>
		</entry>
		<entry>
			<ind>-20</ind>
			<dep>0.010</dep>
		</entry>
		<entry>
			<ind>0</ind>
			<dep>0</dep>
		</entry>
		<entry>
			<ind>20</ind>
			<dep>-0.010</dep>
		</entry>
		<entry>
			<ind>40</ind>
			<dep>-0.020</dep>
		</entry>
		<entry>
			<ind>90</ind>
			<dep>-0.03</dep>
		</entry>
		<entry>
          <ind>180</ind>
          <dep>-0.03</dep>
        </entry>
	</interpolation>
	<axis> 
		<x>0</x>
		<y>0</y>
		<z>1</z>
	</axis>
</animation>
<animation> 
	<type>translate</type>
	<object-name>wingline</object-name>
	<property>orientation/pitch-deg</property>
	<interpolation> 
		<entry>
			<ind>-180</ind>
			<dep>0.03</dep>
		</entry>
		<entry>
			<ind>-90</ind>
			<dep>0.03</dep>
		</entry>
		<entry>
			<ind>-40</ind>
			<dep>0.020</dep>
		</entry>
		<entry>
			<ind>-20</ind>
			<dep>0.010</dep>
		</entry>
		<entry>
			<ind>0</ind>
			<dep>0</dep>
		</entry>
		<entry>
			<ind>20</ind>
			<dep>-0.010</dep>
		</entry>
		<entry>
			<ind>40</ind>
			<dep>-0.020</dep>
		</entry>
		<entry>
			<ind>90</ind>
			<dep>-0.03</dep>
		</entry>
		<entry>
          <ind>180</ind>
          <dep>-0.03</dep>
        </entry>
	</interpolation>
	<axis> 
		<x>0</x>
		<y>0</y>
		<z>1</z>
	</axis>
</animation>
  </PropertyList>
