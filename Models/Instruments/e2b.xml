<?xml version="1.0"?>

<!-- E2B Standby Compass -->

<PropertyList>

<path>e2b.ac</path>

<animation>
	<type>rotate</type>
	<object-name>segments</object-name>
	<property>instrumentation/magnetic-compass/indicated-heading-deg</property>
	<center>
		<x-m>2.99046</x-m>
		<y-m>0.158092</y-m>
		<z-m>1.19359</z-m>
	</center>
	<axis>
		<x>0</x>
		<y>0</y>
		<z>1</z>
	</axis>
</animation>

<!-- *** LoD *** -->
<animation>
	<type>range</type>
	<min-m>0</min-m>
	<max-m>15</max-m>
</animation>

</PropertyList>
