<?xml version="1.0"?>

<PropertyList>
  <path>fuel.ac</path>

  <animation>
    <type>rotate</type>
    <object-name>needle</object-name>
    <property>/consumables/fuel/group4-lbs</property>
    <interpolation>
      <entry><ind>0</ind><dep>-215</dep></entry>
      <entry><ind>10000</ind><dep>-90</dep></entry>
      <entry><ind>20000</ind><dep>35</dep></entry>
    </interpolation>
    <axis>
      <x>0</x>
      <y>0</y>
      <z>-1</z>
    </axis>
  </animation>
  
  <!-- *** LoD *** -->
  <animation>
    <type>range</type>
    <min-m>0</min-m>
    <max-m>5</max-m>
  </animation>
  
</PropertyList>
