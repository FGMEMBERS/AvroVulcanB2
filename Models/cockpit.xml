<!-- All the objects are listed here, and are only visible when in the cockpit -->
<animation>
  <name>InteriorGroup</name>
  <object-name>Cockpit</object-name>
  <object-name>PanelSurround</object-name>
  <object-name>Seat1</object-name>
  <object-name>Seat2</object-name>
  <object-name>SidePanel1</object-name>
  <object-name>SidePanel2</object-name>
  <object-name>Joystick1</object-name>
  <object-name>Joystick2</object-name>
  <object-name>JoystickMount1</object-name>
  <object-name>JoystickMount2</object-name>
  <object-name>ThrottleSurround</object-name>
  <object-name>Throttle1</object-name>
  <object-name>Throttle2</object-name>
  <object-name>Throttle3</object-name>
  <object-name>Throttle4</object-name>
</animation>

<!-- Throttles -->
 <animation>
  <type>rotate</type>
  <object-name>Throttle1</object-name>      
  <property>controls/engines/engine[0]/throttle</property>
  <factor>-75</factor>
  <offset-deg>0</offset-deg>   
  <axis>
   <x1-m>0.42</x1-m>
   <y1-m>-1.0</y1-m>
   <z1-m>0.37</z1-m>  
   <x2-m>0.42</x2-m>
